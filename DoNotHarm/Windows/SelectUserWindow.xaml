<Window x:Class="DoNotHarm.Windows.SelectUserWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DoNotHarm.Windows"
        mc:Ignorable="d"
        Title="SelectUserWindow" Height="100" Width="200">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition />
            <RowDefinition />
        </Grid.RowDefinitions>
        <ComboBox x:Name="UsersComboBox" SelectionChanged="UsersComboBox_SelectionChanged"></ComboBox>
        <Button Grid.Row="1" Click="SelectButton_Click">Выбрать</Button>
    </Grid>
</Window>
